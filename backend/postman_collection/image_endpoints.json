{
  "item": [
    {
      "name": "Generate Image",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{accessToken}}",
            "type": "text",
            "description": "Optional - will associate the image with the authenticated user"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"prompt\": \"A beautiful landscape with mountains and a lake at sunset\",\n  \"userId\": \"{{userId}}\",\n  \"width\": 512,\n  \"height\": 512\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{baseUrl}}/api/images/generate",
          "host": ["{{baseUrl}}"],
          "path": ["api", "images", "generate"]
        },
        "description": "Generate an image using Replicate AI and save it to S3. If userId is provided or user is authenticated, the image will be associated with the user."
      },
      "response": []
    },
    {
      "name": "Get User Images",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{accessToken}}",
            "type": "text"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/api/images/user",
          "host": ["{{baseUrl}}"],
          "path": ["api", "images", "user"]
        },
        "description": "Get all generated images for the authenticated user"
      },
      "response": []
    },
    {
      "name": "Delete Image",
      "request": {
        "method": "DELETE",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{accessToken}}",
            "type": "text"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/api/images/:imageId",
          "host": ["{{baseUrl}}"],
          "path": ["api", "images", ":imageId"],
          "variable": [
            {
              "key": "imageId",
              "value": "{{imageId}}",
              "description": "The ID of the image to delete"
            }
          ]
        },
        "description": "Delete a generated image associated with the authenticated user"
      },
      "response": []
    }
  ]
}
